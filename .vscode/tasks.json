	"version": "2.0.0",
	"tasks": [
		{
			"label": "Install",
			"type": "shell",
			"command": "npm",
			"args": [
				"install",
			],
			"group": "none",
			"problemMatcher": []
		},
		{
			"label": "Build (Good)",
			"type": "shell",
			"command": "npm",
			"args": [
				"run",
				"build"
			],
			"group": "build",
			"problemMatcher": []
		},
		{
			"label": "Build (Bad)",
			"type": "shell",
			"command": "npx",
			"args": [
				"@11ty/eleventy"
			],
			"group": "build",
			"problemMatcher": []
		}
	]
}
